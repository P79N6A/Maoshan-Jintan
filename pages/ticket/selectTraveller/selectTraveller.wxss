page {
  background-color: #efefef;
}

.add-news {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 580rpx;
  height: 75rpx;
  margin: 40rpx auto 0 auto;
  background-color: #fff;
  border-radius: 20rpx;
}

.add-news text {
  font-size: 30rpx;
  color: #00a0e8;
  margin-left: 18rpx;
}

.addImg {
  width: 35rpx;
  height: 35rpx;
  margin-left: 200rpx;
}

.completeInfor {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  height: 135rpx;
  padding-left: 40rpx;
  padding-right: 30rpx;
  margin-top: 50rpx;
  background-color: #fef9e5;
}

.writeImg {
  width: 34rpx;
  height: 32rpx;
}

.selectedImg {
  width: 35rpx;
  height: 28rpx;
}

.travellerInfor {
  display: flex;
  flex-direction: column;
  margin-left: -320rpx;
}

.travellerInfor text {
  font-size: 26rpx;
}

.travellerInfor text:nth-child(1) {
  color: #000101;
}

.travellerInfor text:nth-child(2) {
  color: #979795;
  margin-top: 10rpx;
}

.complete {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #ff9a14;
  color: #fff;
  font-size: 35rpx;
  position: fixed;
  bottom: 0;
}
